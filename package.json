{
  "name": "alba-lucia-gonzalez-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract:i18n": "formatjs extract \"{pages,components,sections}/**/*.{js,jsx,ts,tsx}\" --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file content/locales/es.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple content/locales content/compiled-locales",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "moment": "^2.29.3",
    "next": "^12.1.0",
    "react": "17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-calendly": "^4.0.1",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.31.1",
    "react-intl": "^6.0.5"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.0.2",
    "babel-plugin-formatjs": "^10.3.25",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "sass": "^1.50.0"
  }
}
